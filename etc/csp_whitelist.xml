<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="script-src">
            <values>
                <value id="blpaczka-sandbox" type="host">https://sandbox.blpaczka.com</value>
                <value id="blpaczka-production" type="host">https://send.blpaczka.com</value>
            </values>
        </policy>
        <policy id="frame-src">
            <values>
                <value id="blpaczka-sandbox" type="host">https://sandbox.blpaczka.com</value>
                <value id="blpaczka-production" type="host">https://send.blpaczka.com</value>
            </values>
        </policy>
        <policy id="connect-src">
            <values>
                <value id="blpaczka-sandbox" type="host">https://sandbox.blpaczka.com</value>
                <value id="blpaczka-production" type="host">https://send.blpaczka.com</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
